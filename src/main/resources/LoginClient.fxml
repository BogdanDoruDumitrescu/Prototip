<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: cadetblue" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fis.maven.Controllers.LoginClient">
    <children>
        <TextField fx:id="UsenameField" layoutX="215.0" layoutY="158.0" />
        <PasswordField fx:id="PasswordField" layoutX="215.0" layoutY="206.0" />
        <Label fx:id="user" layoutX="102.0" layoutY="156.0" style="-fx-background-color: aliceblue" text="Username:">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label fx:id="pass" layoutX="102.0" layoutY="204.0" style="-fx-background-color: aliceblue" text="Password:">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Button layoutX="146.0" layoutY="260.0" mnemonicParsing="false" onAction="#login" prefHeight="26.0" prefWidth="256.0" style="-fx-background-color: aquamarine" text="Sign in" />
        <Label fx:id="id" alignment="CENTER" layoutX="149.0" layoutY="87.0" prefHeight="30.0" prefWidth="158.0" style="-fx-background-color: darkorange" text="Login as client">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Button layoutX="40.0" layoutY="53.0" mnemonicParsing="false" onAction="#butonBack" text="Back" />


    </children>
</AnchorPane>
